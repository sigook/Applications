<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"] Sigook</title>
</head>
<body style="margin:0;padding:0;background-color:#FFF;font-family:system-ui, Arial, Helvetica, sans-serif;font-size: 18px; line-height: 1.4">
    <center>
        <div style="padding: 50px 20px 20px;">

            <div style="font-family: Impact;padding: 10px 15px 35px;font-size: 24px;letter-spacing: 1px;text-transform: uppercase; margin-bottom: 15px">
                @await Html.PartialAsync("_NotificationsSigookLogo")
            </div>

            @RenderBody()

            <div style="padding: 20px 20px 30px; margin-top: 15px">
                <div style="font-weight: 200; font-size: 14px; margin-bottom:20px;">
                    <p style="margin:0">GET THE SIGOOK APP</p>
                </div>
                @await Html.PartialAsync("_AppStoreLogo")
                @await Html.PartialAsync("_PlayStoreLogo")
            </div>
            <div style="font-family: inherit; text-align: center">
                <span style="box-sizing: border-box; margin: 0;padding: 0;outline: initial none initial;font-weight: 400; overflow-wrap: break-word; word-break: break-word; color: #4a4a4a; font-style: italic; font-variant-ligatures: normal; font-variant-caps: normal; letter-spacing: normal; orphans: 2; text-align: start; text-indent: 0; text-transform: none; white-space: normal; widows: 2; word-spacing: 0; -webkit-text-stroke-width: 0; background-color: rgb(255, 255, 255); text-decoration-thickness: initial; text-decoration-style: initial; text-decoration-color: initial; font-size: 12px">SIGOOK&trade; is dba of COVENANT GROUP INVESTORS LLC</span>
            </div>
        </div>
    </center>
</body>
</html>