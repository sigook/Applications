@{
    Layout = null;
}
@using Covenant.HtmlTemplates.Views.Shared
@model Covenant.HtmlTemplates.Views.Billing.Payroll.PayrollEmailViewModel
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Payroll</title>
</head>
<body  style="font-family: 'Open Sans', 'Helvetica Neue', sans-serif; box-sizing: border-box; padding: 30px; font-size: 14px;">
    <div>
        <p> Hi @Model.WorkerFullName,</p>
        <p> You are receiving an EFT payment of @Model.TotalNet.ToUsMoney() of the week ended on @Model.EndDate.ToString("M"). Funds will be accredited in your account any time on @Model.PaymentDate.ToString("D") Attached we send the pay stub of this payment.
        </p>
        <p>Thank you very much for your work and have a nice weekend,</p>
    </div>
    <div style="margin-top: 15px; color: #203864; line-height: 18px;">
        <span style="color: #0070c0;">Payroll Covenant</span> <br>
        Off.  647 660 4440 <br>
        Mob.  647 428 7692 <br>
        <a href="https://www.covenantgroupl.com" target="_blank">www.covenantgroupl.com</a> <br>
        6625 Kitimat Rd. Unit 45 MISSISSAUGA, ON L5N 6J1<br>
        <a href="mailto:payroll@covenantgroupl.com" style="color: #0563c1;">payroll@covenantgroupl.com</a> <br>
    </div>

    <div style="margin-top: 20px;">
        <img src="data:image/jpeg;base64," alt="logo">
    </div>

    <div>
        <p style="font-size: 11px; font-style: italic; line-height: 16px">This message (including any attachments) may contain information that is privileged or confidential and is intended for a specific individual and purpose, and is protected by law.  If you are not the intended recipient, please notify the sender and delete this message from your system.  Any disclosure, copying, or distribution, of this message, or the taking of any action based on it, is strictly prohibited.</p>
    </div>
</body>
</html>